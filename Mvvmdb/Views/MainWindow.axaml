<Window
    x:Class="Mvvmdb.Views.MainWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="using:Avalonia.Xaml.Interactivity"
    xmlns:ia="using:Avalonia.Xaml.Interactions.Core"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:Mvvmdb.ViewModels"
    Title="Mvvmdb"
    Initialized=""
    d:DesignHeight="450"
    d:DesignWidth="800"
    DataContext="{Binding ResultViewModel, Source={StaticResource ServiceLocator}}"
    Icon="/Assets/avalonia-logo.ico"
    mc:Ignorable="d">

    <i:Interaction.Behaviors>
        <ia:EventTriggerBehavior EventName="Initialized" >
            <ia:InvokeCommandAction Command="{Binding OnInitializedCommand}" />
        </ia:EventTriggerBehavior>
    </i:Interaction.Behaviors>

    <ItemsControl ItemsSource="{Binding PoetryCollenction}">
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <TextBlock Text="{Binding Name}" />
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>


</Window>
